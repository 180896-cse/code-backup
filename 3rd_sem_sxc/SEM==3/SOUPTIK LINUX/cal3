clear
echo -n "Enter Roll="
read roll
echo -n "Enter total="
read total
grade=A
	if [ $total -ge 60 -a $total -lt 70 ]
	then
	grade=B
	fi
	if [ $total -ge 50 -a $total -lt 60 ]
        then
        grade=C
        fi
	if [ $total -ge 40 -a $total -lt 50 ]
        then
        grade=D
        fi
	if [ $total -lt 40 ]
        then
	grade=F
	fi
echo "Roll no is $roll , total is $total and grade is $grade"

    		
