REM
CLS
INPUT "ENTER SIZE"; S
DIM A(S, S)
FOR I = 1 TO S
FOR J = 1 TO S
INPUT "ENTER ELEMENTS"; A(I, J)
NEXT J
NEXT I
PRINT "ORIGINAL MATRIX:-"
FOR I = 1 TO S
FOR J = 1 TO S
PRINT A(I, J); " ";
NEXT J
PRINT
NEXT I
PRINT "LEFT DIAGONAL:-"
FOR I = 1 TO S
J = A(I, I)
F = 0
FOR K = 2 TO J - 1
IF J MOD K = 0 THEN
F = 1
K = J
END IF
NEXT K
IF F = 0 THEN
PRINT "PRIME="; A(I, I)
END IF
NEXT I
PRINT "RIGHT DIAGONAL:-"
T = S
F = 0
FOR I = 1 TO S
FOR K = 2 TO A(I, T) - 1
IF A(I, T) MOD K = 0 THEN
F = 1
K = A(I, T)
END IF
NEXT K
IF F = 0 THEN
PRINT "PRIME="; A(I, T)
END IF
T = T - 1
NEXT I
END





