REM
CLS
PRINT "PRESS 1 FOR FACTORIAL"
PRINT "PRESS 2 FOR CALCULATING POWER OF A NUMBER"
INPUT "ENTER NUMBER"; N
INPUT "ENTER YOUR CHOICE"; CH
IF CH = 1 THEN GOSUB 10
IF CH = 2 THEN GOSUB 20
END
10 S = 1
FOR I = 1 TO N STEP 1
S = S * I
NEXT I
PRINT "FACTORIAL"; S
RETURN
20 INPUT "NO.", A
INPUT "POWER"; P
V = A ^ P
PRINT "REQD.RESULT"; V
RETURN

