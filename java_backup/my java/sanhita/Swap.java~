import java.util.*;
class Swap
{
    public static void main(String args[])
    {
        int a,b,c=0;
        Scanner sc=new Scanner(System.in);
        System.out.println("enter two numbers");
        a=sc.nextInt();
        b=sc.nextInt();
        a=;
        c=b;
        b=a;
        System.out.println("swapped number="+a);
        System.out.println("swapped number="+b);
    }
}

        