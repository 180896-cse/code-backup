import java.util.*;
class Frequency
{
    public static void main()
    {
        int i,n,k,f;
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter the nos.:");
        n=sc.nextInt();
        int a[]=new int[n];
        for(i=0;i<n;i++)
        a[i]=sc.nextInt();
        System.out.println("Enter the nos to check frequency:");
        k=sc.nextInt();
        f=0;
        for(i=0;i<n;i++)
        if(a[i]==k)
        f++;
        System.out.println("Frequency of element:"k+"is:"+f);
    }
}