class test2
{
    public static void main(String[]args)
    {
        
        
        for(int j=1;j<=5;j++)
        {
           System.out.print(j);
           for(int i=2;i<=5;i++)
           {
             for(int j=1;j<=(i-1);j++)
               System.out.print(j);  
               for(int j=1;j<=(5-1);j++)
                System.out.print("*");
                System.out.println("");
            }
        }
    }
}
                
               