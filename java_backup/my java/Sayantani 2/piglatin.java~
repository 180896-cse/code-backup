        int i,t;
        t=0;
        for ( i=0;i<s.length();i++)
        if(s.charAt(i)=='a' || s.charAt(i)=='e' || s.charAt(i)=='i' || s.charAt(i)=='o' || s.charAt(i)=='u')
        {
             t =i;
          break;
        }
           for(i=t;i<s.length();i++)
           System.out.print(s.charAt(i));
           for(i=0;i<t;i++)
           
           System.out.println(s.charAt(i));
