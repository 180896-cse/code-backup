import java.io.*;
class co
{
    public static void main(int n)throws IOException
    {
        int a[]=new int[n];
        for(int i=0;i<n;i++)
        a[i]=i+1;
        int c=2;
        for(int i=0;i<n;i++)
        
        {
            int b[]=new int[n];
            for(int j=0;j<n;j+=c)
            {
                b[j]=a[i];
                System.out.print(b[j]);
            }
            System.out.println("");
            c++;
        }
    }
}