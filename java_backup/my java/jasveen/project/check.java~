package project;

class check
{
    public static void main(int a,int b,int c)
    {
        int m;
        switch(c)
        {
            case'+':m=a+b;
            System.out.println(m);
            break;
            case'-':m=a-b;
            System.out.println(m);
            break;
            case'/':m=a/b;
            System.out.println(m);
            break;
            case'*':m=a*b;
            System.out.println(m);
            break;
            default:System.out.println("error");
        }
    }
}
    