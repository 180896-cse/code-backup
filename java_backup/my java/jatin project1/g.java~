import java.io.*;
 class lowercase_uppercase
    {
        public static void main(String args[])throws IOException
            {
                int i,m=0;
                 String s;
                 InputStreamReader ISR=new InputStreamReader(System.in);
                 BufferedReader br=new BufferedReader(ISR);
                  System.out.println("Give string:");
                 s=br.readLine();
                String  w1="";
                
                for(i=0;i<s.length();i++)
                    {
                        m=(int)s.charAt(i); 
                        if(m>=65&&m<=90)
                        w1=w1+(char)(m+32);
                        //w=w1.toLowerCase;
                        else
                        if(m>=97&&m<=122)
                         w1=w1+(char)(m-32);
                        //w=w2.toUpperCase;
                    }
                    System.out.print(w1);
                }
            }
