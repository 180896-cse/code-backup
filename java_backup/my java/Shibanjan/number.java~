import java.io.*;
class saddle1
{
      public static void main(String arg[])throws IOException
      {
          BufferedReader br=new BufferedReader(new InputStreamReader(System.in))
          double n;
          System.out.println("Enter a number not more than 8 digits");
          n=Integer.parseInt(br.readLine())
          double dup=n,r,i=0,c,j=0;
          String a[]=new String[10];
          do
          {
              r=dup%10;
              c++;
              if(r==1)
              {
                  a[i]="one";
              }
              if(r==2)
              {
                  a[i]="two";
              }
              if(r==3)
              {
                  a[i]="three";
              }
              if(r==4)
              {
                  a[i]="four";
              }
              if(r==5)
              {
                  a[i]="five";
              }
              if(r==6)
              {
                  a[i]="six";
              }
              if(r==7)
              {
                  a[i]="seven";
              }
              if(r==8)
              {
                  a[i]="eight";
              }
              if(r==9)
              {
                  a[i]="nine";
              }
              if(r==0)
              {
                  a[i]="zero";
              }
              i++;
              dup=dup/10;
            }while(dup!=0);
            if(c==8)
            {
                System.out.print(a[j]+"crore");
                j++;c--;
            }
                        if(c==7)
            {
                System.out.print(a[j]+"ty"+a[j+1]+"lakhs");
                j++;c=c+2;
            }          
            if(c==6)
            {
                System.out.print(a[j]+"lakhs");
                j++;c--;
            }
            if(c==5)
            {
                System.out.print(a[j]+"ty"+a[j+1]+"thousand");
                j++;c=c+2;
            }           
            if(c==4)
            {
                System.out.print(a[j]+"thousand");
                j++;c--;
            }            
            if(c==3)
            {
                System.out.print(a[j]+"hundred");
                j++;c--;
            }            
            if(c==2)
            {
                System.out.print(a[j]+"ty");
                j++;c--;
            }            
            if(c==1)
            {
                System.out.print(a[j]);
                j++;c--;
            }
        }
    }