import java.io.*;
class number7
{
    public static void main (int n) throws IOException
    {
        int i,j;
        BufferedReader br=new BufferedReader (new InputStreamReader(System.in));
        System.out.println("Enter a number");
        
        for(i=1;i<=n;i++)
         {
             for(j=1;j<=i;j++)
               System.out.print(j);
             for(j=(i-1);j>=1;j--)
               System.out.print(i-1); 
             System.out.println("");
            }
        }
    }
        
        