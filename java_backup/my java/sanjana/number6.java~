import java.io.*;
class number6
{
    public static void main(int n)throws IOException
    {
        int i, j;
        BufferedReader br=new BufferedReader(new InputStreamReader(System.in));
        System.out.println("Enter a number");
        i=0;
        for(i=5; i<=1; i--)
        {
            for(j=5; j<=i; j++)
            System.out.print("*");
            System.out.println("");
        }
    }
}
        
            
       