class even
{
    public static void main(int a,int b,int c)
    {
        if((a-b)%2==0)
        System.out.print("even");
        else
        System.out.print("odd");
        if((b-c)%2==0)
        System.out.print("even");
        else
        System.out.print("odd");
        if((c-a)%2==0)
        System.out.print("even");
        else
        System.out.print("odd");
    }
}
