import java.util.*;
class Reverse
{
public static void main (String s [])
{
int num;
int rev;
int dig;
Scanner sc =new Scanner (System.in);
System.out.println ("Enter the number ");
num=sc.nextInt();
int copy =num ;
rev=0;
while (Copy>0)
{
dig=Copy%10;
rev=(rev*10)+dig;
Copy=Copy/10;
}
System.out.println("Reverse "+rev);
}
}