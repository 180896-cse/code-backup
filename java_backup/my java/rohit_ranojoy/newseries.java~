import java.io.*;
public class newseries
{
    public static void main(String args[])throws IOException
    {
        int i;int j;
        for(i=1;i<=5;i++)
        {
            for(j=1;j<=i;j++)
            {
                System.out.print(i);
            }
             System.out.println(" ");
            }
        }
    }
    
        
            