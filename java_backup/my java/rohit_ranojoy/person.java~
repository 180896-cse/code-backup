public class check
{
    public static void main()
    {
        String name="rohit";
        int age=4
        if(age>=18)
        {
            System.out.println("major"+"can drive"+age);
        }
        else
        {
            System.out.println("minor"+"cannot drive"+age);
            
        }
        System.out.println("name="+name);
        System.out.println("age="+age);
    }
}
}
        