public class calculate
{
    public static void main(int a,int b)
    {
        int c;
        c=a+b;
        system.out.println("addition="+c);
        c=a-b;
        system.out.println("substraction="+c);
        c=a/b;
        system.out.println("division="+c);
        c=a*b;
        system.out.println("multiplication="+c);
    }
}
      
    