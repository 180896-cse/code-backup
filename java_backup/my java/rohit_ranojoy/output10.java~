public class output10
{
    public static void main(int x,int y)
    {
        x=19;
        y=11;
        System.out.println(++x+y++);
        System.out.println(++x+ ++y);
        System.out.print(++x+ ++y);
        System.out.println(--x+--y);
        System.out.println(-x+-y);
        System.out.println(+x+y);
    }
}
        
