
class number1 extends abs
{
    int a,b,i,j,s,k,m,f,c;
    number1()
    {
        a=0;
        b=0;
    }
    void input(int x,int y)
    {
        a=x;
        b=y;
    }
    void display()
    
    {
        f=1;m=0;k=0;s=0;
        do
        {
            a=sum(a);
        }
            while(a>9);
            if(a==1)
            {
                k=1;
            }
            else 
            k=0;
            for(i=b;i>0;i=i/10)
            {
                c=i%10;
                for(j=1;j<=c;j++)
                {
                    f=f*j;
                    s=s+f;
                }
            }
            if(s==b)
            {
                m=1;
            }
            else 
            m=0;
            if(k==1)
            {
                System.out.println("lucky number"+a);
            }
            if(m==1)
            {
                System.out.println("krishnamurty number"+b);
            }
        }
        void cal()
        {
            display();
        }
    }
                
                    
            
        
        