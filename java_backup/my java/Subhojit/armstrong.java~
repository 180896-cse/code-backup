import java.util.*;
class armstrong
{
    public static void main(String args[])
    {
        Scanner Sc=new Scanner(System.in);
        int n, k, s=0;
        System.out.println("Enter any no.");
        n=Sc.nextInt();
        while(n>0)
        {
            k=n%10;
            s=s+(k*k*k);
            n=n/10;
        }
        if(s==n)
        {
            System.out.println(n+"is an armstrong no.");
        }
        else
        {
            System.out.println(n+"is not an armstrong no.");
        }
    }
}
