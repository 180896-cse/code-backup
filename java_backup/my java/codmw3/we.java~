import java.util.*;
public class we
{
    void main()
    {
        Scanner sc=new Scanner(System.in);
        
        int a,aa=0,c,d,e=0,m,n,i,j,g,k,p;
        int l=0;
        System.out.println("Enter limit");
        a=sc.nextInt();
        
        int b[]=new int[a];
        d=0;
        for(c=0;c<a;c=c+1)
        {
            System.out.println("Enter Number");
            b[c]=sc.nextInt(); 
        }
        e=b[0]+1;
        for(l=0;l<a;l=l+1)
        {
            if(b[l]>e)
            {
                e=b[l]+1;
            }
            }
     
        System.out.println(+e);
            
        
        }
    }
    