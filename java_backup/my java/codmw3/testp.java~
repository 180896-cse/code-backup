import java.util.*;
public class testp
{
    void main()
    {
        Scanner sc=new Scanner(System.in);
        int a,b,c;
        System.out.println("Enter number");
        a=sc.nextInt();
        
        for(b=1;b<=a;b++)
        {
            for(c=1;c<=a*2;c++)
            {
                System.out.print("*");
            }
            System.out.println();
        }
    }
}