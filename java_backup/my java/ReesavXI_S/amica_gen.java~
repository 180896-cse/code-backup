import java.util.*;
class amica_gen
{
    void calc()
    {
        amicable obj=new amicable();
        int sum=0;
        for(int i=1;i<1000;i++)
        {
            sum=0;
            for(int j=1;j<i;j++)
            {
                if(i==j)
                sum=sum+j;
            }
            obj.amicable(i,sum);
        }
    }
}