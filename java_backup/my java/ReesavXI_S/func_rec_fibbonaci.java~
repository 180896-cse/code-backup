import java.util.*;
class func_rec_fibbonaci
{
    int n,i;
    func_rec_fibbonaci(int num)
    {
        n=num;
        i=0;
    }
    void fibb(int i)
    {
        if(i==n)
        {
            System.out.print(n);
            return;
        }
        else
        {
            System.out.print(i);
            fibb((i+i));
        }
    }
}
