import java.util.*;
class number_2_star
{
int num;
number_2_star(int n)
{
num=n;
}
void cal()
{
    int a=0,s=0;
    while(num>0)
    {
        a=num%10;
        s=(s*10)+a;
        num=num/10;
    }
    while(s>0)
    {
        a=s%10;
        if(a==1)
        {
        System.out.print("         *           ");   
        System.out.print("        **           ");
        System.out.print("       * *           ");
        System.out.print("      *  *           ");
        System.out.print("         *           ");
        System.out.print("         *           ");
        System.out.print("         *           ");
        System.out.print("   *************     ");
    }
    else if(a==2)
    {
        System.out.print("  ************       ");
        System.out.print("             *       ");
        System.out.print("             *       ");
        System.out.print("  ************       ");
        System.out.print("  *                  ");
        System.out.print("  *                  ");
        System.out.print("  *                  ");
        System.out.print("  ************       ");
    }
    else if(a==3)
    {
        System.out.print("  ************       ");
        System.out.print("             *       ");
        System.out.print("             *       ");
        System.out.print("  ************       ");
        System.out.print("  ************       ");
        System.out.print("             *       ");
        System.out.print("             *       ");
        System.out.print("  ************       ");
    }
    else if(a==4)
    {
       System.out.print("  *                  ");
       System.out.print("  *                  ");
       System.out.print("  *        *         ");
       System.out.print("  *        *         "); 
       System.out.print("  ************       ");
       System.out.print("           *         ");
       System.out.print("           *         ");
       System.out.print("           *         ");
    }
    else if(a==5)
    {
        System.out.print("  ************       ");
        System.out.print("  *                  ");
        System.out.print("  *                  ");
        System.out.print("  *                  ");
        System.out.print("  ************       ");
        System.out.print("             *       ");
        System.out.print("             *       ");
        System.out.print("  ************       ");
    }
    else if(a==6)
    {
       System.out.print("   ***********       ");
        System.out.print("  *                  ");
        System.out.print("  *                  ");
        System.out.print("  *                  ");
        System.out.print("  ************       ");
        System.out.print("  *          *       ");
        System.out.print("  *          *       ");
        System.out.print("  ************       ");
    } 
    else if(a==7)
    {
        System.out.print("  ************       ");
        System.out.print("             *       ");
        System.out.print("             *       ");
        System.out.print("             *       ");
        System.out.print("      ***********    ");
        System.out.print("             *       ");
        System.out.print("             *       ");
        System.out.print("             *       ");
    }
    else if(a==8)
    {
        System.out.print("  ************       ");
        System.out.print("  *          *       ");
        System.out.print("  *          *       ");
        System.out.print("  ************       ");
        System.out.print("  ************       ");
        System.out.print("  *          *       ");
        System.out.print("  *          *       ");
        System.out.print("  ************       ");
    }
    else if(a==9)
    {
       System.out.print("  ************       ");
       System.out.print("  *          *       ");
       System.out.print("  *          *       ");
       System.out.print("  ************       ");
       System.out.print("             *       ");
       System.out.print("             *       ");
       System.out.print("             *       ");
       System.out.print("  ************       ");
    } 
     else
     {
      System.out.print("  ************       ");
      System.out.print("  *          *       ");
      System.out.print("  *          *       ");
      System.out.print("  *          *       ");
      System.out.print("  *          *       ");
      System.out.print("  *          *       ");
      System.out.print("  *          *       ");
      System.out.print("  ************       ");
    } 
    s=s/10;
}
}
}  
       