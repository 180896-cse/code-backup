class product
{
    public static void main()
    {
        int a,b,c;
        a=20;
        b=9;
        c=a*b;
        System.out.println(c);
    }
}
class divide
{
    public static void main()
    {
        int a,b,c;
        a=20;
        b=9;
        c=(a+b)/2;
        system.out.println(c);
    }
}

        
        
