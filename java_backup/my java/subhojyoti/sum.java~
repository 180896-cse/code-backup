class sum
{
    public static void main()
    {
        int a,b,c;
        a=25;
        b=17;
        c=a+b;
        system.out.println(c);
    }
}
class product
{
    public static void main()
    {
        int a,b,c;
        a=25;
        b=17;
        c=a*b;
        system.out.println(c);
    }
}
class subtract
{
    public static void main()
    {
        int a,b,c;
        a=25;
        b=17;
        c=a-b;
        system.out.println(c);
    }
}

        
 

