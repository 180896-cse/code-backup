

class test565
{
   public static void main()
   {
       for(int i=1;i<=5;i++)
       {
           for(int j=1;j<=i;j++)
           {
               
               System.out.print(0);
            }
               System.out.print((j*i)+" ");
           
            }
             System.out.println();
            }
    }

