

 class TEST2
{
   public static void main(int n)
   {
       int i,j,k,k1;
       k=5;k1=1;
       for(i=1;i<=5;i++)
       {
           for(j=1;j<=k;j++)
           System.out.print(j);
           System.out.println(" ");
           for(j=1;j<=k1;j--);
           System.out.print(j);
           k++;
           k1--;
           System.out.println(" ");
        }
           
              
       
    }
}
