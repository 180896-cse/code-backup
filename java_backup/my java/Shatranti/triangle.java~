public class triangle
{
    public static void main(int a,int b)
    {
        int c,l;
        System.out.println("Enter first side");
        System.out.println("Enter second side");
        System.out.println("Enter third side");
        c=(l*a*b)/2;
        System.out.println("area="+c);
    }
}
    
    
    