import java.io.*;
class aismita
{
    public static void main(String s)
    {
        String w="";
        for(int i=0;i<s.length();i++)
        {
            int m=(int)s.charAt(i);
            if(m>=65 && m<=90)
            w=w+Character.toLowerCase((char)m);
            else if(m>=97 && m<=122)
             w=w+Character.toUpperCase((char)m);
             //System.out.println(w);
            
        }
    }    System.out.println(w);
}