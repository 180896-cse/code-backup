import java.util.*;
class prime
{
    public static void main(string args[])
    int i, j, f=1;
    Scanner sc=new Scanner(System.in);
    void Input()
    {
        check();
    }
    void check()
    {
        for(i=1; i<=100; i++)
        {
            f=0;
            for(j=2; j<i; j++)
            if(i%j==0)
            {
                break;
            }
            if(f==0)
            System.out.println(i);
        }
    }
}

   
    
                