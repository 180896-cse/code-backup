
public class pr
{
    public static void main(String args[])
    {
        kbc abc = new kbc();
        abc.delay();
        abc.disp();
        for(int i = 1;i<=10;i++)
        {
            abc.delay();//delay loop.
        }
        System.out.println("\f");
    }
    public void disp()
    {
        System.out.println();
        System.out.println();
        System.out.println();
        System.out.println();
        System.out.println();
        System.out.println();
        System.out.println();
        System.out.println();
        System.out.println();
        System.out.println();
        System.out.println();
        System.out.println();
        System.out.println();
        System.out.println();
        System.out.println();
        System.out.println();
        System.out.println();
        System.out.println();
        System.out.println("                                                                                      ***    *****            ********               *******     ");
        System.out.println("                                                                                      ***   ***               ***   ****          ***            ");
        System.out.println("                                                                                      ***  **                 ***   ****          ***            ");
        System.out.println("                                                                                      ******                  *******             ***            ");
        System.out.println("                                                                                      ***  **                 ***   ****          ***            ");
        System.out.println("                                                                                      ***   ***        ***    ***   ****    ***    ***           ");
        System.out.println("                                                                                      ***    *****     ***    ********      ***      *******     ");
    }
    public void delay()
    {
        for(long i = 1;i<=2000000000;i++){}        
    }
}
